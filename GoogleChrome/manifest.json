{
	"name": "ColorSpace",
	"version": "0.1",
	"manifest_version": 2,
	"description": "This is extension for Google Chrome. It let you to easily find color value in hex, rgb and hsv with it's name.",
	
	"browser_action": {
	    "default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
    "web_accessible_resources": [
    "ColorDictionary.json"],

	"background": { "scripts": ["js/Colors.js", "jquery/jquery-1.7.1.min.js", "js/background.js"] },
  	"icons": {
    "128": "icon.png"
  	}
}